{
  "name": "next-app-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build || exit 0",
    "build:ignore-errors": "set NEXT_IGNORE_TYPE_ERROR=1 && next build",
    "export": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx -c .eslintrc.json --fix --ignore-pattern 'example/**'",
    "lint:fix": "eslint . --ext .ts,.tsx -c .eslintrc.json --fix --ignore-pattern 'example/**'",
    "lint:strict": "eslint . --ext .ts,.tsx -c .eslintrc.json --max-warnings=0 --ignore-pattern 'example/**'",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path .prettierignore",
    "type-check": "tsc --noEmit",
    "check": "node scripts/code-quality.js",
    "check:eslint": "node scripts/code-quality.js --only=eslint",
    "check:types": "node scripts/code-quality.js --only=types",
    "check:deps": "node scripts/code-quality.js --only=deps",
    "report": "node scripts/generate-report.js",
    "report:eslint": "node scripts/generate-report.js --type=eslint",
    "report:complexity": "node scripts/generate-report.js --type=complexity",
    "report:deps": "node scripts/generate-report.js --type=deps",
    "clean": "rimraf .next out",
    "serve-static": "npx serve out",
    "prepare": "husky install"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@heroui/button": "2.2.16",
    "@heroui/code": "2.2.12",
    "@heroui/input": "2.4.16",
    "@heroui/kbd": "2.2.12",
    "@heroui/link": "2.2.13",
    "@heroui/listbox": "2.3.15",
    "@heroui/navbar": "2.2.14",
    "@heroui/react": "^2.7.5",
    "@heroui/snippet": "2.2.17",
    "@heroui/switch": "2.2.14",
    "@heroui/system": "2.4.12",
    "@heroui/theme": "2.4.12",
    "@iconify/react": "^5.2.1",
    "@react-aria/ssr": "3.9.7",
    "@react-aria/visually-hidden": "3.8.20",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.8",
    "axios": "^1.8.4",
    "clsx": "2.1.1",
    "framer-motion": "11.13.1",
    "intl-messageformat": "^10.5.0",
    "next": "15.0.4",
    "next-themes": "^0.4.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-draggable": "^4.4.6"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.12",
    "@next/eslint-plugin-next": "15.0.4",
    "@react-types/shared": "3.25.0",
    "@types/node": "20.5.7",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "8.11.0",
    "@typescript-eslint/parser": "8.11.0",
    "autoprefixer": "10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-no-unsanitized": "^4.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-unused-imports": "4.1.4",
    "lint-staged": "^15.5.1",
    "npm-check": "^6.0.1",
    "postcss": "8.4.49",
    "prettier": "3.3.3",
    "rimraf": "^5.0.5",
    "tailwind-variants": "0.3.0",
    "tailwindcss": "3.4.16",
    "typescript": "5.6.3"
  }
}